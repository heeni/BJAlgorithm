//1000
//두 정수 A와 B를 입력받은 다음, A+B를 출력하는 프로그램을 작성하시오.
package day1;

import java.util.Scanner;

public class day1_02 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a, b;
		a = sc.nextInt();
		b = sc.nextInt(); 
		
		if(0 < a && b < 10) {
			System.out.println(a+b);
		}
	}
}
